    "peer-info": "^0.6.2",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "async": "^2.0.0-rc.4",
    "debug": "^2.2.0",
    "heap": "^0.2.6",
    "highland": "^3.0.0-beta.1",
